{
  "manifest_version": 2,

  "name": "Kaskus Utilities (BETA)",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "0.1",
  "options_page": "options.html",
  "content_scripts": [{
    "matches": ["http://www.kaskus.co.id/*"],
    "css": ["dist/assets/css/main.css"],
    "js": ["jquery.js", "plugin.js", "dist/assets/js/main.js", "option.js"]
  }],
  "permissions": [
    "http://www.kaskus.co.id/",
    "tabs",
    "storage",
    "http://fiddle.jshell.net/"
  ]
}
